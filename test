


        <script>
            // Get the canvas element
        const ctx = document.getElementById('myChart').getContext('2d');

        // Chart data
        const data = {
        labels: ["NLUC", "MLUC", "SLUC", "OUS"],
        datasets: [{
            label: 'Data',
            data: [1, 30, 40, 10],
            backgroundColor: [
                'rgba(255, 99, 132, 0.2)',
                'rgba(255, 159, 64, 0.2)',
                'rgba(255, 205, 86, 0.2)',
                'rgba(75, 192, 192, 0.2)',
            ],
            borderColor: [
                'rgb(255, 99, 132)',
                'rgb(255, 159, 64)',
                'rgb(255, 205, 86)',
                'rgb(75, 192, 192)',
            ],
            borderWidth: 2,
        }]
        };

        // Chart configuration
        const options = {
        scales: {
            responsive:true,
            x: {
            beginAtZero: true
            }
        }
        };

        // Create Chart.js bar chart
        const myChart = new Chart(ctx, {
        type: 'bar',
        data: data,
        options: options
        });
        </script>
